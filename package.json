{
  "name": "hadrientequi",
  "version": "0.0.0",
  "description": "site 11",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "start": "concurrently \"npm:watch:css\" \"npm:watch:js\" \"npm:watch:eleventy\"",
    "format": "prettier . --write",
    "build:css": "postcss src/styles/style.css -o _build/style.css",
    "watch:css": "postcss src/styles/style.css -o _build/style.css --watch",
    "build:js": "rollup -c rollup.config.js",
    "watch:js": "rollup -c rollup.config.js --watch",
    "build:eleventy": "npx @11ty/eleventy",
    "watch:eleventy": "npx @11ty/eleventy --serve"
  },
  "keywords": [],
  "author": "Achtaitaipai",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "rollup": "^3.25.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  }
}
